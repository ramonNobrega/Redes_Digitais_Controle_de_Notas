<name>calculaMediaBimestral</name>
<type>pre</type>
<connectionType>pool</connectionType>
<transact>false</transact>
<businessProcess></businessProcess>
<triggerTemplate></triggerTemplate>
<triggerCode>public void calculaMediaBimestral() {
			DesempenhoBimestral item = getBean();
			item.setMediaBimestre((item.getNota1() + item.getNota2() + item.getNota3())/3);
			this.desempenhoBimestralBC.update(getBean());
	}
</triggerCode>
<triggerCallCode>calculaMediaBimestral();
</triggerCallCode>
